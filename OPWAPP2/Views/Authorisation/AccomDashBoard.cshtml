@model OPWAPP2.Models.Authorisation

@{
    ViewBag.Title = "AccomDashBoard";
}

<body>
    <H2> Accommodation User DashBoard </H2>


    @if (Session["UserName"] != null)
    {
        <text> Welcome Accommodation Staff -  @Session["UserName"].ToString() </text>

    }
    <br />
    <h4>List of Tasks</h4> <img class="mySlides" src="~/Resources/Building Approval.png" alt="AMD Image" style="width:50%">
    <hr />

    <div>
        <ol>
            <li>
                List DEASP Elective works Awaiting approval <button type="button" onclick="location.href='@Url.Action("AccomWorksforApproval","Work")'">New Works Request</button>
                <br /><br />
            </li>
            <li>
                List DEASP Elective works Awaiting Funding <button type="button" onclick="location.href='@Url.Action("AccomWorksforFunding","Work")'">New Works Request</button>
                <br /><br />
            </li>
            <li>
                status of All DEASP Elective works requests <button type="button" onclick="location.href='@Url.Action("DEASPWorksStatus","Work")'">New Works Request</button>
                <br /><br />
            </li>
        </ol>
    </div>
</body>


